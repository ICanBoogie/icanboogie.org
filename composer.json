	"description": "Starter project for your application built with ICanBoogie",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/app-hello/issues",
		"source": "https://github.com/ICanBoogie/app-hello"
